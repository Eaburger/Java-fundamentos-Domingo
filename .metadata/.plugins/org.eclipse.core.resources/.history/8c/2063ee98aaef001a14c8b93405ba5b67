package br.com.fundamentos;

public class Escape {
		
	public static void main(String[] args) {
		System.out.println("alicio da \"cunha\"");
		System.out.println("alicio da \ncunha");
		System.out.println("alicio da \bcunha");
		System.out.println("alicio da \tcunha");
		System.out.println("alicio da \fcunha");
		System.out.println("alicio da \rcunha");
		System.out.println("alicio da \\cunha");
		
	}
}
